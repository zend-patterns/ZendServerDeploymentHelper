{
    "name": "zend-pattern/ZendServerDeploymentHelper",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "psr/log": "^1.0",
        "psr/log-implementation": "^1.0",
        "symfony/filesystem": "^3.0",
        "symfony/process": "^3.0",
        "zendframework/zend-diactoros": "^1.3",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-servicemanager": "^3.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "zendframework/zend-config-aggregator": "^1.0",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^6.0.3",
        "monolog/monolog": "^1.8",
        "zendframework/zend-log": "^2.7"
    },
    "suggest": {
        "zendframework/zend-config-aggregator": ""
    },
    "autoload": {
        "psr-4": {
            "ZendServer\\DepH\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendServerTest\\DepH\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=auto --no-coverage",
        "test-coverage": "phpunit --colors=auto --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    }
}
